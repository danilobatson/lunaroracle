{
  "name": "lunaroracle",
  "version": "0.1.0",
  "description": "AI agent that predicts crypto price movements using social sentiment data",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npx tsc",
    "type-check": "npx tsc --noEmit",
    "db:migrate": "wrangler d1 execute lunarcrush-universal-db --file=database/schema.sql",
    "db:local": "wrangler d1 execute lunarcrush-universal-db --local --file=database/schema.sql"
  },
  "keywords": [
    "crypto",
    "ai",
    "predictions",
    "social-sentiment",
    "lunarcrush"
  ],
  "author": "Danilo Batson",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250810.0",
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.18.1",
    "@types/jest": "^30.0.0",
    "hono": "^4.9.0",
    "lunarcrush-sdk": "^3.5.0",
    "typescript": "^5.9.2"
  }
}
